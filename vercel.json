{
  "build": {
    "env": {
      "NODE_VERSION": "18.x",
      "PNPM_VERSION": "9.4.0"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": ""
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": ""
        }
      ]
    }
  ]
}
